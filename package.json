{
  "name": "upflow",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-p build:*",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development bun run build:server --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks/index.cjs ./build/server.js",
    "format": "prettier --write .",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks/index.cjs --require dotenv/config ./build/server.js",
    "test": "bun test ",
    "test:e2e:dev": "cross-env NODE_ENV=development playwright test --config=playwright.dev.config.ts",
    "test:e2e:run": "cross-env NODE_ENV=production CI=true playwright test --config=playwright.config.ts",
    "typecheck": "tsc -b",
    "validate": "run-p test lint typecheck"
  },
