{
  "name": "upflow",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.js",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest --run",
    "test:playwright": "cross-env TEST=true playwright test",
    "typecheck": "tsc -b",
    "validate": "run-p test lint typecheck"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.9",
    "@chakra-ui/react": "^2.3.2",
    "@chakra-ui/styled-system": "^2.3.1",
    "@chakra-ui/system": "^2.2.9",
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@gitbeaker/node": "^35.7.0",
    "@googleapis/sheets": "^3.0.2",
    "@prisma/client": "^4.3.1",
    "@remix-run/express": "^1.7.0",
    "@remix-run/node": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "@remix-run/serve": "^1.7.0",
    "@remix-run/server-runtime": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "cleye": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "framer-motion": "^7.3.2",
    "globby": "^13.1.2",
    "google-auth-library": "^8.5.1",
    "got": "^12.4.1",
    "morgan": "^1.10.0",
    "node-schedule": "^2.1.0",
    "octokit": "^2.0.7",
    "prisma": "^4.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "remeda": "^1.1.0",
    "tiny-invariant": "^1.2.0",
    "ts-pattern": "^4.0.5",
    "tsx": "^3.9.0",
    "zod": "^3.19.0",
    "zod-form-data": "^1.2.1"
  },
  "devDependencies": {
    "@octokit/plugin-rest-endpoint-methods": "^6.5.0",
    "@octokit/types": "^7.3.0",
    "@playwright/test": "^1.25.2",
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "^8.4.6",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.16",
    "@types/node-schedule": "^2.1.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.23.1",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "esbuild": "^0.15.8",
    "esbuild-plugin-clean": "^0.9.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^6.0.4",
    "msw": "^0.47.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.23.1"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
