{
  "name": "upflow",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-p build:*",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development pnpm run build:server --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks/index.cjs ./build/server.js",
    "format": "prettier --write .",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks/index.cjs --require dotenv/config ./build/server.js",
    "test": "vitest --run",
    "test:e2e:dev": "cross-env NODE_ENV=development playwright test --config=playwright.dev.config.ts",
    "test:e2e:run": "cross-env NODE_ENV=production CI=true playwright test --config=playwright.config.ts",
    "typecheck": "tsc -b",
    "validate": "run-p test lint typecheck"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/react": "^2.7.0",
    "@chakra-ui/styled-system": "^2.9.1",
    "@chakra-ui/system": "^2.5.8",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@gitbeaker/node": "^35.8.1",
    "@googleapis/sheets": "^4.0.2",
    "@octokit/plugin-rest-endpoint-methods": "^7.1.2",
    "@prisma/client": "^4.15.0",
    "@remix-run/express": "^1.16.1",
    "@remix-run/node": "^1.16.1",
    "@remix-run/react": "^1.16.1",
    "@remix-run/serve": "^1.16.1",
    "@remix-run/server-runtime": "^1.16.1",
    "@remix-validated-form/with-zod": "^2.0.5",
    "@tanstack/react-query": "^4.29.12",
    "bcryptjs": "^2.4.3",
    "cleye": "^1.3.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.1.3",
    "express": "^4.18.2",
    "framer-motion": "^10.12.16",
    "globby": "^13.1.4",
    "google-auth-library": "^8.8.0",
    "got": "^13.0.0",
    "ky": "^0.33.3",
    "morgan": "^1.10.0",
    "node-schedule": "^2.1.1",
    "octokit": "^2.0.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "remeda": "^1.19.0",
    "remix-auth": "^3.4.0",
    "remix-auth-form": "^1.3.0",
    "remix-validated-form": "^4.6.11",
    "tiny-invariant": "^1.3.1",
    "ts-pattern": "^4.3.0",
    "tsx": "^3.12.7",
    "zod": "^3.21.4",
    "zod-form-data": "^2.0.1"
  },
  "devDependencies": {
    "@octokit/types": "^9.2.3",
    "@playwright/test": "^1.34.3",
    "@remix-run/dev": "^1.16.1",
    "@remix-run/eslint-config": "^1.16.1",
    "@tanstack/react-query-devtools": "^4.29.12",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "^8.40.0",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@types/node-schedule": "^2.1.0",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.31.3",
    "binode": "^1.0.5",
    "c8": "^7.14.0",
    "esbuild": "^0.17.19",
    "esbuild-plugin-clean": "^1.0.1",
    "eslint": "^8.41.0",
