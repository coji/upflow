{
  "name": "upflow",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development pnpm run build:server --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.js",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest --run",
    "test:e2e:dev": "playwright test",
    "test:e2e:run": "cross-env CI=true playwright test",
    "typecheck": "tsc -b",
    "validate": "run-p test lint typecheck"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.12",
    "@chakra-ui/react": "^2.4.1",
    "@chakra-ui/styled-system": "^2.3.5",
    "@chakra-ui/system": "^2.3.3",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@gitbeaker/node": "^35.8.0",
    "@googleapis/sheets": "^4.0.0",
    "@octokit/plugin-rest-endpoint-methods": "^6.7.0",
    "@prisma/client": "^4.6.1",
    "@remix-run/express": "^1.7.6",
    "@remix-run/node": "^1.7.6",
    "@remix-run/react": "^1.7.6",
    "@remix-run/serve": "^1.7.6",
    "@remix-run/server-runtime": "^1.7.6",
    "@remix-validated-form/with-zod": "^2.0.5",
    "@tanstack/react-query": "^4.16.1",
    "bcryptjs": "^2.4.3",
    "cleye": "^1.3.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "framer-motion": "^7.6.7",
    "globby": "^13.1.2",
    "google-auth-library": "^8.7.0",
    "got": "^12.5.3",
    "ky": "^0.32.2",
    "morgan": "^1.10.0",
    "node-schedule": "^2.1.0",
    "octokit": "^2.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "remeda": "^1.3.0",
    "remix-validated-form": "^4.6.4",
    "tiny-invariant": "^1.3.1",
    "ts-pattern": "^4.0.6",
    "tsx": "^3.12.1",
    "zod": "^3.19.1",
    "zod-form-data": "^1.2.4"
  },
  "devDependencies": {
    "@octokit/types": "^8.0.0",
    "@playwright/test": "^1.28.0",
    "@remix-run/dev": "^1.7.6",
    "@remix-run/eslint-config": "^1.7.6",
    "@tanstack/react-query-devtools": "^4.16.1",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/node-schedule": "^2.1.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.25.2",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "esbuild": "^0.15.14",
    "esbuild-plugin-clean": "^1.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^7.7.0",
    "msw": "^0.49.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "prisma": "^4.6.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.25.2"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
