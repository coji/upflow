{
  "name": "upflow",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:batch": "pnpm tsx build.ts",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:playwright": "cross-env TEST=true playwright test",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@chakra-ui/icons": "^2.0.9",
    "@chakra-ui/react": "^2.3.1",
    "@chakra-ui/styled-system": "^2.3.1",
    "@chakra-ui/system": "^2.2.9",
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@gitbeaker/node": "^35.7.0",
    "@ladjs/graceful": "^3.0.2",
    "@prisma/client": "^4.3.1",
    "@remix-run/express": "^1.7.0",
    "@remix-run/node": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "@remix-run/serve": "^1.7.0",
    "@remix-run/server-runtime": "^1.7.0",
    "@tanstack/react-table": "^8.5.13",
    "@tanstack/react-virtual": "3.0.0-alpha.0",
    "bcryptjs": "^2.4.3",
    "bree": "^9.1.2",
    "cleye": "^1.2.1",
    "compression": "^1.7.4",
    "dayjs": "^1.11.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "framer-motion": "^7.2.1",
    "globby": "^13.1.2",
    "got": "^12.4.1",
    "morgan": "^1.10.0",
    "octokit": "^2.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "remeda": "^1.1.0",
    "simple-node-logger": "^21.8.12",
    "tiny-invariant": "^1.2.0",
    "ts-pattern": "^4.0.5",
    "tsx": "^3.9.0",
    "zod": "^3.19.0",
    "zod-form-data": "^1.2.1",
    "zx": "^7.0.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@octokit/plugin-rest-endpoint-methods": "^6.4.1",
    "@octokit/types": "^7.2.0",
    "@playwright/test": "^1.25.1",
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "^8.4.6",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.15",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.23.1",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.7",
    "esbuild-plugin-clean": "^0.9.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^6.0.4",
    "msw": "^0.47.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "prisma": "^4.3.1",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2",
    "vite": "^3.1.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.23.1"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
