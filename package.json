    "happy-dom": "^9.20.3",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.8",
    "prisma": "^4.15.0",
    "remix-flat-routes": "^0.5.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.3",
    "vitest-environment-vprisma": "^1.2.0"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  }
}
