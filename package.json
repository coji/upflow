{
  "name": "upflow",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:batch": "pnpm esbuild batch/cycletime.ts --bundle --platform=node --target=node16.6 --outfile=cycletime.js --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@chakra-ui/react": "^2.2.4",
    "@emotion/cache": "^11.9.3",
    "@emotion/react": "^11.9.3",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.9.3",
    "@gitbeaker/node": "^35.7.0",
    "@prisma/client": "^4.1.0",
    "@remix-run/node": "^1.6.5",
    "@remix-run/react": "^1.6.5",
    "@remix-run/serve": "^1.6.5",
    "@remix-run/server-runtime": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "cleye": "^1.2.1",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "framer-motion": "^6.5.1",
    "globby": "^13.1.2",
    "got": "^12.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.2.0",
    "tsx": "^3.8.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@remix-run/dev": "^1.6.5",
    "@remix-run/eslint-config": "^1.6.5",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "^8.4.5",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "binode": "^1.0.5",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "cypress": "^10.3.1",
    "esbuild": "^0.14.50",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^6.0.4",
    "msw": "^0.44.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "prisma": "^4.1.0",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.19.1"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node --require tsconfig-paths/register prisma/seed.js"
  }
}
