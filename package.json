    "octokit": "3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.10.1",
    "remeda": "1.24.0",
    "remix-auth": "3.5.0",
    "remix-auth-form": "1.3.0",
